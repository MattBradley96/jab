
	>>> from jab import jab

	>>> def test(a,b):
	...     result, error = jab(a).apply(b)
	...     if result: show(result)
	...     else: show(error)

	>>> test('*','asterisk')
	'*'

	>>> test('\talan\n','indented_text_line')
	[1, 'alan']

	>>> test('    >>> from test_file import TestFile\n','indented_line')
	[1, '>>> from test_file import TestFile']

	>>> test('\n','eol')
	1
	>>> test('''line one
	... second line
	... line three''','lines')
	['line one', 'second line']

	>>> test('isexsl','not_x')
	'ise'

	>>> test('isexsl','up_to_x')
	'ise'
