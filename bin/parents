#! /usr/bin/env python3

"""Remove an option from args"""

from pysyte.cli.main import run
from pysyte.types import paths


def add_args(parser):
    parser.positional('paths')


def main(args):
    lines = '\n'.join(paths.path(args.paths[0]).parent_directories())
    print(lines)
    return True


run(main, add_args=add_args)
