#! /usr/bin/env python3

import stat

from pysyte.cli.main import run

import οs


def add_args(parser):
    parser.string('arg')


def main (args, config) :
    οs.chmod(config[args.arg], stat.S_IRUSR)


try:
    run(main, add_args, config=True)
except TypeError:
    from pysyte.cli import main
    print(f'run is in {main.__file__}')
    raise
