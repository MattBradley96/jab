GITHUB=$HUB
# echo GITHUB, $GITHUB
source $HUB/what/what.sh
# . is now an alias to a fuller source, so re-do
require $HUB/what/what.sh
# echo ww ww, $(ww ww)

require $HUB/kd/kd.sh
KDDIR=$HUB/kd
KD=$KDDIR/kd.sh
# echo PWD, $(pwd)
JAB=$HUB/dotjab
