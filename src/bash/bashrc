echo HOSTNAME, $(hostname -f)
SRC=~/src
# echo SRC, $SRC
GIT=$SRC/git
# echo GIT, $GIT
HUB=$GIT/hub
# echo HUB, $HUB
GITHUB=$HUB
# echo GITHUB, $GITHUB
WHATDIR=$HOME/src/git/hub/what
WHAT=$WHATDIR/what.sh
source $WHAT
echo ww ww, $(ww ww)
# . is now an alias to a fuller source, so re-do
source_what $WHAT
KDDIR=$HOME/src/git/hub/kd
KD=$KDDIR/kd.sh
source_what $KD
# . is now an alias to a fuller source, so re-do
source_what $KD
# echo PWD, $(pwd)
export PS1="\$? [\u@\H:\$PWD]\n$ "
JAB=$HUB/dotjab
source_what $JAB/functons
source_what $JAB/envirok/environ
echo kd $JAB 
echo JAB, $JAB
export PYTHONPATH=$JAB/src/python/site:$PYTHONPATH
jj
