#! /bin/sh

kd ()
{ 
    cd `python ~/.jab/python/kd.py $*`
}

ru ()
{
	if [ -z "$*" ]
	then 
		sudo -i
	else
		sudo $*
	fi
}

fsh ()
{
	if [ -f fred.sh ]
	then
		sh fred.sh
	else
		sh fred
	fi
}

fpy ()
{
	if [ -f fred.py ]
	then
		p fred.py
	else
		p fred
	fi
}

which ()
{ 
    ( alias; declare -f ) | \
		$WHICH --tty-only --read-alias --read-functions --show-tilde --show-dot $@
}
